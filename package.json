{
  "name": "next-tester",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "esbuild": "^0.16.11",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest",
    "webpack": "^5.75.0"
  }
}
